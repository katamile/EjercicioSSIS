         *  string filePath = (string)rawConnection;
         *  //Use the connection in some code here, then release the connection
         *  Dts.Connections["Prices.zip"].ReleaseConnection(rawConnection);
         * */
        #endregion


        class Cliente
        {

            public string nombre { get; set; }
            public string apellido { get; set; }
            public int edad { get; set; }
            public string domicilio { get; set; }
            public string correo { get; set; }

            public Cliente(
                string name, string lastname, int age, string domicilio, string correo)
            {

                this.nombre = name;
                this.apellido = lastname;
                this.edad = age;
                this.domicilio = domicilio;
                this.correo = correo;

            }

        }

        public void Main()
		{
            List<Cliente> clientList = new List<Cliente>();
            Dictionary<string, Object> data = new Dictionary<string, object>();
            data.Add("totalFacturas", (decimal)Dts.Variables["User::TotalFacturas"].Value);

            DataTable registroClientes = new DataTable();
            OleDbDataAdapter oleDA = new OleDbDataAdapter();
            oleDA.Fill(registroClientes, Dts.Variables["User::Clientes"].Value);

            if (registroClientes.Rows.Count > 0)
            {

                foreach (DataRow cliente in registroClientes.Rows)
                {
                    clientList.Add(new Cliente(
                        (string)cliente["nombre"],
                        (string)cliente["apellido"],
                        (int)cliente["edad"],
                        (string)cliente["domicilio"],
                        (string)cliente["correo"]
                    ));
                }

                data.Add("clientes", clientList);

            }

            try
            {

                HttpClient client = new HttpClient();

                var dataString = new StringContent(
                    JsonSerializer.Serialize(data),
                    System.Text.Encoding.UTF8,
                    "application/json"
                );

                var response = client.PostAsync(
                    (string)Dts.Variables["User::MainUrl"].Value,
                    dataString
                ).Result;

                if ((int)response.StatusCode == 200)
                {
                    Dts.TaskResult = (int)ScriptResults.Success;
                }
                else
                {
                    Dts.TaskResult = (int)ScriptResults.Failure;
                }

            }
            catch (Exception e)
            {
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
